      "@babel/preset-env": "^7.19.0",
      "@babel/preset-react": "^7.18.6",
      "@rollup/plugin-babel": "^5.3.1",
      "npm-run-all": "^4.1.5",
      "rollup": "^2.79.0",
      "rollup-plugin-delete": "^2.0.0",
      "rollup-plugin-peer-deps-external": "^2.2.4"
  },
  "peerDependencies": {
