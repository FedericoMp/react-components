{
  "name": "react-components",
  "version": "1.0.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "dependencies": {
      "react-scripts": "5.0.1"
  },
  "scripts": {
      "build": "rollup -c",
